{
    "name": "quillstack/local-storage",
    "description": "The library to manage files in local storage.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "quillstack",
        "storage",
        "php8",
        "local"
    ],
    "homepage": "https://quillstack.org/storage",
    "authors": [
        {
            "name": "Radek Ziemniewicz",
            "email": "radek@quillstack.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Quillstack\\LocalStorage\\": "src/",
            "Quillstack\\LocalStorage\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "quillstack/storage-interface": "^0.1.0"
    },
    "require-dev": {
        "quillstack/unit-tests": "^0.2.4"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        }
    }
}
